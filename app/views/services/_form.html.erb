<%= simple_form_for(service) do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :category, collection: Service::CATEGORIES %>
  <%= f.input :price %>
  <%= f.input :photos, as: :file, input_html: { multiple: true }%>
  <%= f.submit class: "btn btn-dark w-100" %>
<% end %>
